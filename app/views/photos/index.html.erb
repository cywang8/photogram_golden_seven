<h1>All Photos</h1>

<div>
  <% @list_of_photos.each do |photo| %>
    <img src="<%= photo.source %>">
    <p>
      <%= photo.caption %>
    </p>

    <div>
      <button><a href ="https://photogram-golden-seven-cywang89.c9users.io/photos/<%= photo.id %>">Detail</a></button>
    </div>  
    
    <div>
      <button><a href ="https://photogram-golden-seven-cywang89.c9users.io/photos/<%= photo.id %>/edit">Edit</a></button>
    </div>
     
    <div>
      <button><a href ="https://photogram-golden-seven-cywang89.c9users.io/photos/delete_photo/<%= photo.id %>">Delete</a></button>
    </div>
    
    <br>
    <br>
    
  <% end %>
  
</div>